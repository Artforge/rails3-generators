class <%= plural_name.camelize %>::Edit < Mustache::Rails
  
  <% for attribute in attributes -%>
  def <%= attribute.name %>
    <%= singular_name %>.<%= attribute.name %>
  end
  
  <% end -%>

  def show_path
    <%= singular_name %>_path(@<%= singular_name %>)
  end
  
  def index_path
    <%= plural_name %>_path
  end
  
end